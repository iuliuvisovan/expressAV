      function showCdDetail(index) {
        var cd = cdData[index];
        var html = '<div class="cd-detail-content">';

        html += '<div class="cd-detail-image">';
        html += '<div class="cd-spine"></div>';
        html += '<img src="' + cd.image + '" alt="' + cd.name + '" />';
        html += '</div>';

        html += '<h3 class="cd-title">' + cd.name + '</h3>';
        html += '<p class="cd-year">' + cd.year + '</p>';

        if (cd.trailer) {
          html += '<div class="cd-trailer-box">';
          html += '<h4 class="cd-trailer-title">Trailer</h4>';
          html += '<div class="cd-trailer">';
          html +=
            '<iframe src="' +
            cd.trailer +
            '" title="Trailer" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe>';
          html += '</div>';
          html += '</div>';
        }

        html += '<div class="cd-links">';
        for (var i = 0; i < cd.links.length; i++) {
          html += '<a href="' + cd.links[i].url + '" target="_blank" class="cd-link">' + cd.links[i].label + '</a>';
        }
        html += '</div>';
        html += '</div>';

        document.getElementById('cd-detail').innerHTML = html;
      }

      document.addEventListener('DOMContentLoaded', function () {
        var cdItems = document.querySelectorAll('.cd-item');

        cdItems.forEach(function (item) {
          item.addEventListener('click', function () {
            cdItems.forEach(function (i) {
              i.classList.remove('active');
            });
            this.classList.add('active');
            showCdDetail(parseInt(this.getAttribute('data-cd')));
          });
        });

        // Show first CD by default
        showCdDetail(0);
      });
    </script>

    <!-- Events Section -->
    <section id="events" class="bg-light-gold">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 text-center">
            <h2 class="section-heading">Events</h2>
            <h3 class="section-subheading text-muted">Aurelia Vișovan's upcoming events</h3>
          </div>
        </div>
        <div class="row">
          <div class="panel panel-default">
            <table class="table table-hover">
              <thead>
                <tr>
                  <th>Name</th>
                  <th>Date</th>
                  <th>City</th>
                  <th>Location</th>
                </tr>
              </thead>
              <div class="panel-heading">Upcoming (click on an event to see the programme)</div>
              <tbody>
                <% data.concerts .filter(function (item) { return item.date * 1000 >= (+new Date() - 86400000); }).sort(function (a, b) {
                return a.date - b.date }).forEach(function(item) { %>
                <tr class="clickable" data-toggle="collapse" data-target="#concertinfo<%= item.date %>">
                  <th scope="row"><%= item.name %></th>
                  <td><%= dateformat(new Date(item.date * 1000), "dd.mm.yyyy") %></td>
                  <td style="white-space: nowrap">
                    <img
                      src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7"
                      style="margin-top: -4px; margin-right: 4px"
                      class="flag flag-<%= item.flag %>"
                    />
                    <%= item.city %>
                  </td>
                  <td><%= item.location %></td>
                </tr>
                <tr class="collapse" id="concertinfo<%= item.date %>">
                  <td colspan="8" style="background-color: #f9f9f9"><b></b><%- item.programme %></td>
                </tr>
                <% }); %>
              </tbody>
            </table>
          </div>
          <div class="panel panel-default" style="max-height: 400px; overflow: auto">
            <div class="panel-heading">Archive</div>
            <table class="table table-hover">
              <thead>
                <tr>
                  <th>Name</th>
                  <th>Date</th>
                  <th>City</th>
                  <th>Location</th>
                </tr>
              </thead>
              <tbody>
                <% data.concerts .filter(function (item) { return item.date * 1000 < (+new Date() - 86400000); }).sort(function (a, b) {
                return b.date - a.date }).forEach(function(item) { %>
                <tr>
                  <td><%= item.name %></td>
                  <td><%= dateformat(new Date(item.date * 1000), "dd.mm.yyyy") %></td>
                  <td>
                    <img
                      src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7"
                      style="margin-top: -4px"
                      class="flag flag-<%= item.flag %>"
                    />
                    <%= item.city %>
                  </td>
                  <td><%= item.location %></td>
                </tr>
                <% }); %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </section>

    <!-- Media Section -->
    <section id="media">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 text-center">
            <h2 class="section-heading">Media</h2>
            <h3 class="section-subheading text-muted">Photo gallery and recordings</h3>
          </div>
        </div>
        <div class="row">
          <div id="myCarousel" class="carousel slide">
            <ol class="carousel-indicators">
              <% data.mediaPhotos.forEach(function(item, i) { %>
              <li data-target="#myCarousel" data-slide-to="<%= i %>" class="<%= i == 0 ? 'active' : '' %>"></li>
              <% }) %>
            </ol>

            <div class="carousel-inner">
              <% data.mediaPhotos.forEach(function(item, i) { %>
              <div class="item <%= i == 0 ? 'active' : '' %>">
                <img
                  src="<%= item.url %>"
                  alt="<%= item.alt %>"
                  title="<%= item.title %>"
                  class="img-responsive"
                  style="max-height: 780px; margin: auto"
                  loading="lazy"
                />
              </div>
              <% }) %>
            </div>

            <a class="carousel-control left" href="#myCarousel" data-slide="prev">
              <span class="icon-prev"></span>
              <div class="invistext">Previous</div>
            </a>

            <a class="carousel-control right" href="#myCarousel" data-slide="next">
              <span class="icon-next"></span>
              <div class="invistext">Next</div>
            </a>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-12">
            <ul class="timeline">
              <% var right = false; data.mediaVideos.sort((a,b) => b.year - a.year).forEach(function (item) { %>
              <li class="<%= right ? 'timeline-inverted' : '' %>">
                <div class="timeline-image">
                  <!-- <img class="img-circle img-responsive" src="<%= item.imageUrl %>"
                                 alt="Aurelia Visovan in <%= item.year %>"> -->
                </div>
                <div class="timeline-panel">
                  <div class="timeline-heading">
                    <h4 class="subheading"><%= item.title %></h4>
                  </div>
                  <div class="embed-responsive embed-responsive-<%= item.ratio %>">
                    <iframe width="420" height="315" src="<%= item.url %>" allowfullscreen></iframe>
                  </div>
                </div>
              </li>
              <% right = !right }) %>
              <li class="timeline-image">
                <div class="timeline-image">
                  <h5>More <br />To <br />Come</h5>
                </div>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </section>

