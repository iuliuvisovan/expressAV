    <!-- Header -->
    <header>
      <div class="container">
        <div class="intro-text">
          <h1 class="main-title">Aurelia Visovan</h1>
          <a href="#about" style="margin-bottom: 100px" class="page-scroll btn btn-xl" title="Learn more about Aurelia">Enter</a>
          <center>
            <div class="news" style="display: none">
              <h3>News:</h3>
              <div class="date"><span class="glyphicon glyphicon-star-empty"></span><br /><span class="hidden-xs">30.01.2015</span></div>
              <h4>
                The trio Aurelia Visovan(piano), Vera Karner(clarinet) and Dominik Wagner(double bass) won the "Fanny Mendelssohn
                Förderpreis" in Hamburg and will<br class="visible-xs" />
                record their debut CD for Berlin Classics
              </h4>
            </div>
          </center>
        </div>
      </div>
    </header>

    <!-- News Section -->
    <section id="news">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 text-center">
            <h2 class="section-heading">Latest News</h2>
            <h3 class="section-subheading text-muted" style="margin-bottom: 0">Aurelia Vișovan's Masterclasses in 2025</h3>
          </div>
        </div>
        <div class="row text-center">
          <div class="col-md-12">
            <iframe
              src="https://www.facebook.com/plugins/post.php?href=https%3A%2F%2Fwww.facebook.com%2Faurelia.visovan%2Fposts%2Fpfbid02N9TSrc1tursjtZ1FQdAwNqea8vBYzBqWNaLuA4HAbMbbgTouqVCnDWSctDgzS5TBl&show_text=true&width=500"
              width="750"
              height="500"
              style="border: none; overflow: hidden; margin-top: 24px"
              scrolling="no"
              frameborder="0"
              data-show-text="true"
              allowfullscreen="true"
              allow="autoplay; clipboard-write; encrypted-media; picture-in-picture; web-share"
            ></iframe>
          </div>
        </div>
      </div>
    </section>

    <!-- About Section -->
    <section id="about">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 text-center">
            <h2 class="section-heading">About</h2>
            <h3 class="section-subheading text-muted" style="margin-bottom: 0">Aurelia Vișovan's biography</h3>
          </div>
        </div>
        <div class="row text-center">
          <div class="col-md-12">
            <div class="col-md-9">
              <p class="text-muted" style="text-align: justify; padding-right: 7px">
                <%- [0,1,2].map(x => data.aboutSections[x].content).join('') .replace(/\*\*\*/g, '<br />' + '&nbsp;'.repeat(10)) %>
              </p>
            </div>
            <div class="col-md-3" style="margin-top: 40px">
              <img src="/img/about/about-image.jpg" loading="lazy" alt="Aurelia 2018" style="width: 100%; border-radius: 8px" />
            </div>
          </div>

          <div class="col-md-12 biography-download">
            <div class="row" style="font-size: 19px">
              Download official biography:
              <div style="display: inline-block">
                <a
                  target="_blank"
                  href="https://docs.google.com/document/d/1ksOI1dt21BBhEylxfLv9RsjN6K0L2RDArAZr3BknOVQ/edit"
                  style="margin-left: 5px"
                  ><span class="flag-icon flag-icon-gb" title="English"></span> EN</a
                >
                |
                <a target="_blank" href="https://docs.google.com/document/d/1tRLhDrkplgyGMmw0SqeUKujfyDHVR4MgeyBRaYdvqWs/edit"
                  ><span class="flag-icon flag-icon-de" title="German"></span> DE</a
                >
              </div>
            </div>
            <div class="row" style="font-size: 16px">
              Short version:
              <a
                target="_blank"
                href="https://docs.google.com/document/d/1c3BDyF20dFfyRUtFOqtG4oxxbxL-cfPfFSlnJK-4Gww/edit"
                style="margin-left: 5px"
                ><span class="flag-icon flag-icon-gb" title="English"></span> EN</a
              >
              |
              <a target="_blank" href="https://docs.google.com/document/d/1gXWiZEWkfy_9QIsQxM2BAf5tdlwTEfkiMItU-GCzMCI/edit"
                ><span class="flag-icon flag-icon-de" title="German"></span> DE</a
              >
            </div>
            <div class="row">
              <i> Disclaimer: The provided biographies must be used in their complete form and unmodified.</i>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- Events Section -->
    <section id="events" class="bg-light-gold">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 text-center">
            <h2 class="section-heading">Events</h2>
            <h3 class="section-subheading text-muted">Aurelia Vișovan's upcoming events</h3>
          </div>
        </div>
        <div class="row">
          <div class="panel panel-default">
            <table class="table table-hover">
              <thead>
                <tr>
                  <th>Name</th>
                  <th>Date</th>
                  <th>City</th>
                  <th>Location</th>
                </tr>
              </thead>
              <div class="panel-heading">Upcoming (click on an event to see the programme)</div>
              <tbody>
                <% data.concerts .filter(function (item) { return item.date * 1000 >= (+new Date() - 86400000); }).sort(function (a, b) {
                return a.date - b.date }).forEach(function(item) { %>
                <tr class="clickable" data-toggle="collapse" data-target="#concertinfo<%= item.date %>">
                  <th scope="row"><%= item.name %></th>
                  <td><%= dateformat(new Date(item.date * 1000), "dd.mm.yyyy") %></td>
                  <td style="white-space: nowrap">
                    <img
                      src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7"
                      style="margin-top: -4px; margin-right: 4px"
                      class="flag flag-<%= item.flag %>"
                    />
                    <%= item.city %>
                  </td>
                  <td><%= item.location %></td>
                </tr>
                <tr class="collapse" id="concertinfo<%= item.date %>">
                  <td colspan="8" style="background-color: #f9f9f9"><b></b><%- item.programme %></td>
                </tr>
                <% }); %>
              </tbody>
            </table>
          </div>
          <div class="panel panel-default" style="max-height: 400px; overflow: auto">
            <div class="panel-heading">Archive</div>
            <table class="table table-hover">
              <thead>
                <tr>
                  <th>Name</th>
                  <th>Date</th>
                  <th>City</th>
                  <th>Location</th>
                </tr>
              </thead>
              <tbody>
                <% data.concerts .filter(function (item) { return item.date * 1000 < (+new Date() - 86400000); }).sort(function (a, b) {
                return b.date - a.date }).forEach(function(item) { %>
                <tr>
                  <td><%= item.name %></td>
                  <td><%= dateformat(new Date(item.date * 1000), "dd.mm.yyyy") %></td>
                  <td>
                    <img
                      src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7"
                      style="margin-top: -4px"
                      class="flag flag-<%= item.flag %>"
                    />
                    <%= item.city %>
                  </td>
                  <td><%= item.location %></td>
                </tr>
                <% }); %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </section>

    <!-- Media Section -->
    <section id="media">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 text-center">
            <h2 class="section-heading">Media</h2>
            <h3 class="section-subheading text-muted">Photo gallery and recordings</h3>
          </div>
        </div>
        <div class="row">
          <div id="myCarousel" class="carousel slide">
            <ol class="carousel-indicators">
              <% data.mediaPhotos.forEach(function(item, i) { %>
              <li data-target="#myCarousel" data-slide-to="<%= i %>" class="<%= i == 0 ? 'active' : '' %>"></li>
              <% }) %>
            </ol>

            <div class="carousel-inner">
              <% data.mediaPhotos.forEach(function(item, i) { %>
              <div class="item <%= i == 0 ? 'active' : '' %>">
                <img
                  src="<%= item.url %>"
                  alt="<%= item.alt %>"
                  title="<%= item.title %>"
                  class="img-responsive"
                  style="max-height: 780px; margin: auto"
                  loading="lazy"
                />
              </div>
              <% }) %>
            </div>

            <a class="carousel-control left" href="#myCarousel" data-slide="prev">
              <span class="icon-prev"></span>
              <div class="invistext">Previous</div>
            </a>

            <a class="carousel-control right" href="#myCarousel" data-slide="next">
              <span class="icon-next"></span>
              <div class="invistext">Next</div>
            </a>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-12">
            <ul class="timeline">
              <% var right = false; data.mediaVideos.sort((a,b) => b.year - a.year).forEach(function (item) { %>
              <li class="<%= right ? 'timeline-inverted' : '' %>">
                <div class="timeline-image">
                  <!-- <img class="img-circle img-responsive" src="<%= item.imageUrl %>"
                                 alt="Aurelia Visovan in <%= item.year %>"> -->
                </div>
                <div class="timeline-panel">
                  <div class="timeline-heading">
                    <h4 class="subheading"><%= item.title %></h4>
                  </div>
                  <div class="embed-responsive embed-responsive-<%= item.ratio %>">
                    <iframe width="420" height="315" src="<%= item.url %>" allowfullscreen></iframe>
                  </div>
                </div>
              </li>
              <% right = !right }) %>
              <li class="timeline-image">
                <div class="timeline-image">
                  <h5>More <br />To <br />Come</h5>
                </div>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </section>

