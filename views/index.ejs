            { label: 'Gramola', url: 'https://www.gramola.at/products/9003643992689' },
            { label: 'Spotify', url: 'https://open.spotify.com/album/54T0OeX4UiBwJmcye46huG' },
            { label: 'Apple Music', url: 'https://music.apple.com/us/album/doktor-lichtenthals-mozart-piano-works/1687527671' },
          ],
        },
        {
          name: 'Capriccio Appassionato',
          year: 2022,
          image: '/img/recordings/Capriccio Appassionato.jpg',
          trailer: 'https://www.youtube.com/embed/UIE5mPOkEqg',
          links: [
            { label: 'Passacaille', url: 'https://passacaille.be/en/product/czerny-schubert-capriccio-apassionato/' },
            { label: 'Spotify', url: 'https://open.spotify.com/album/1Lmp7Bgq2gsxCeFlWlwBNG' },
            {
              label: 'Apple Music',
              url: 'https://music.apple.com/us/album/schubert-czerny-capriccio-appassionato-keyboard-sonatas/1635635903',
            },
          ],
        },
        {
          name: 'Harpsifly',
          year: 2021,
          image: '/img/recordings/Harpsifly.jpg',
          trailer: null,
          links: [
            { label: 'Gramola', url: 'https://www.gramola.at/products/9003643992245' },
            { label: 'Spotify', url: 'https://open.spotify.com/album/7ruqJhXauQzzZhOm4MdJCo' },
            {
              label: 'Apple Music',
              url: 'https://music.apple.com/fm/album/harpsifly-a-story-of-gordon-murray-and-his-instruments/1564151702',
            },
          ],
        },
        {
          name: 'Sonata Lunatica',
          year: 2020,
          image: '/img/recordings/Sonata Lunatica.jpg',
          trailer: 'https://www.youtube.com/embed/JBvy2LBHcnM',
          links: [
            { label: 'Passacaille', url: 'https://passacaille.be/en/product/beethoven-sonata-lunatica/' },
            { label: 'Spotify', url: 'https://open.spotify.com/album/1TW4uV4EjN935aGnM4Sail' },
            {
              label: 'Apple Music',
              url: 'https://music.apple.com/us/album/beethoven-sonatina-lunatica-violin-sonatas-nos-9-10/1538668512',
            },
          ],
        },
        {
          name: 'Mozart Hummel Beethoven – Concerto Sonate Symphony',
          year: 2020,
          image: '/img/recordings/Mozart Hummel Beethoven Concerto Sonate Symphony.jpg',
          trailer: null,
          links: [
            { label: 'Outhere Music', url: 'https://outhere-music.com/en/albums/mozart-hummel-beethoven-concerto-sonate-symphony' },
